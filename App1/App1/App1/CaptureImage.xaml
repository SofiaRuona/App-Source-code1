<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App1.CaptureImage"
             Title="Capture Image">
  
  <StackLayout VerticalOptions="Center">
   
  
<Grid Padding="70"></Grid>
    <Image Source="captureImage.jpg"></Image>
    <Button Text="Go" x:Name="BtnGo" Clicked="BtnGo_OnClicked"></Button>
</StackLayout>
  
</ContentPage>