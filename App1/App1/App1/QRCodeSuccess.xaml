<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App1.QRCodeSuccess"
             Title="QR Code Success">
  

<StackLayout VerticalOptions="Center">
  <Label Text="QR code scanned succussfully." HorizontalTextAlignment="Center"></Label>
 <Grid>
   <StackLayout VerticalOptions="Center">
   <Image Source="success_icon.jpg"></Image>
   <Label Text="Uploaded successfully." HorizontalTextAlignment="Center"></Label>
   <Image Source="hr_line.jpg"></Image>
   <Label Text="Click on GO button." HorizontalTextAlignment="Center"></Label>
     </StackLayout>
 </Grid>
 
<Button Text="Vehicle Search" x:Name="BtnGo" Clicked="BtnGo_OnClicked"/>
</StackLayout>
  
</ContentPage>