<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App1.InsuranceRetakeChoice"
             Title="Insurance Choice">
  
  <StackLayout VerticalOptions="Center">
   
  
<Grid Padding="70"></Grid>
    <Label Text="Do you want voluntary Insurance?"></Label>
    <Button Text="Yes" x:Name="BtnYes" Clicked="BtnYes_OnClicked"></Button>
    <Button Text="No" x:Name="BtnNo" Clicked="BtnNo_OnClicked"></Button>
</StackLayout>
  
</ContentPage>