<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App1.VehicleSearch"
             Title="Vehicle Search">
  

<StackLayout VerticalOptions="Center">
  <Label Text="QR code scanned succussfully." HorizontalTextAlignment="Center"></Label>
 <Grid>
   <StackLayout VerticalOptions="Center">
   <Label Text="Enter vehicle name." HorizontalTextAlignment="Center"></Label>
   <Entry x:Name="VehicleName"></Entry>
     </StackLayout>
 </Grid>
<Grid Padding="30"></Grid>
  <Grid>
    <StackLayout VerticalOptions="Center">
      <Label Text="Select city name." HorizontalTextAlignment="Center"></Label>
      <Picker x:Name="LVCityList"></Picker>
      
    </StackLayout>
  </Grid>
<Button Text="Searh Vehicle" x:Name="BtnSearhVehicle"  Clicked="BtnSearhVehicle_OnClicked"/>
<Button Text="Go" x:Name="BtnGo" Clicked="BtnGo_OnClicked"/>
</StackLayout>
  
</ContentPage>